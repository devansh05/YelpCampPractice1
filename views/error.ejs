<% layout("layouts/boilerplate") %>
<div class="row">
  <div class="col-12">
    <div class="alert alert-danger" role="alert">
      <h4 class="alert-heading">ERROR!</h4>
      <p>
        <%= err.message %><br/>
        Status Code : <%= err.statusCode %>
      </p>
      <hr />
      <p class="mb-0">
        <%= err.stack %>
      </p>
    </div>
  </div>
</div>
